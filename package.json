{
    "name": "dotnet-nuget-explorer",
    "displayName": "Dotnet nuget packaget explorer",
    "description": "Nuget package explorer",
    "version": "0.0.1",
    "publisher": "jcmordan",
    "license": "MIT",
    "engines": {
        "vscode": "^1.25.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "keywords": [
        "C#",
        "F#",
        "Nuget"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "packageExplorer.explore",
                "title": "Package Explorer"
            }
        ],
        "views": {
            "nugetExplorer": [
                {
                    "id": "packageExplorer",
                    "name": "PackageExplorer"
                }
            ]
        },
        "viewsContainers": {
            "activitybar": [
                {
                    "icon": "resources/nuget.svg",
                    "id": "nugetExplorer",
                    "title": "Nuget Explorer"
                }
            ]
        }
    },
    "scripts": {
        "compile": "npx npm-run-all compile:*",
        "compile:extension": "tsc",
        "compile:views": "webpack --mode development",
        "watch": "npx npm-run-all -p watch:*",
        "watch:extension": "tsc --watch",
        "watch:views": "webpack --watch --mode development",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "vscode:prepublish": "yarn compile",
        "package": "vsce package"
    },
    "devDependencies": {
        "@types/mocha": "^2.2.42",
        "@types/node": "^7.0.43",
        "@types/react": "^16.8.20",
        "@types/react-dom": "^16.8.4",
        "css-loader": "^3.0.0",
        "less": "^3.7.0",
        "less-loader": "^4.1.0",
        "node-sass": "^4.12.0",
        "npm-run-all": "^4.1.3",
        "sass-loader": "^7.1.0",
        "style-loader": "^0.23.1",
        "styled-components": "^3.3.3",
        "ts-import-plugin": "^1.5.4",
        "ts-loader": "^4.4.2",
        "tslint": "^5.8.0",
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "webpack": "^4.15.1",
        "webpack-cli": "^3.0.8"
    },
    "dependencies": {
        "antd": "^3.19.3",
        "react": "^16.8.6",
        "react-dom": "^16.8.6"
    }
}
